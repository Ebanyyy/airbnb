<%= form_with model: @listing do |f| %>	

	<%= f.label :title%>
	<p><%= f.text_field :title%></p>

	<%= f.label :photos%>
	<p><%=f.file_field :photos, multiple: true%></p>

	<%= f.label :property_type%>
	<p><%= f.text_field :property_type%></p>

	<%= f.label :location%>
	<p><%= f.text_field :location%></p>

	<%= f.label :rooms%>
	<p><%= f.number_field :rooms%></p>

	<%= f.label :capacity%>
	<p><%= f.number_field :capacity%></p>

	<%= f.label :description%>
	<p><%= f.text_field :description%></p>

	<%= f.label :price%>
	<p><%= f.text_field :price%></p>

	<%= f.label :available%>
	<%= f.check_box :available%>
	<br>
	<p><% if @listing.new_record? %>
		<%= f.submit 'Create'%>
	<%else%>
		<%= f.submit 'Update'%>
	<%end%></p>
<%end%>

<%= link_to 'Back to Listing', listings_path(@listings)%>
	
	